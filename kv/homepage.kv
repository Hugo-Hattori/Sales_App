<HomePage>:
    FloatLayout:
        canvas:
            Rectangle:
                size: self.size
                pos: self.pos
                source: "icones/bg1.png"
        LabelButton:
            pos_hint: {"right": 1,"top": 1}
            size_hint: 1, 0.85
            text: "Homepage - Ir para Configs"
            on_release: app.mudar_tela("configpage")
        Button:
            pos_hint: {"right": 1, "top": 0.15}
            size_hint: 0.33, 0.15
            text: "Ir para Configurações"
            on_release: app.mudar_tela("configpage")